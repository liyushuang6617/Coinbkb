            android:textSize="@dimen/size_14"
            android:textColor="@color/black"
            android:text="--"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="@dimen/size_5">
